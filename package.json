{
  "name": "directorcafe",
  "version": "0.0.1",
  "description": "Extensions for TestCafe allows to log, retry steps, rethrow internal exceptions, etc",
  "scripts": {
    "example": "testcafe \"chrome\" examples/*.spec.js -d",
    "integration": "testcafe \"chrome:headless\" src/__integrationTests__/**/*.spec.js",
    "test": "jest",
    "flow": "flow",
    "lint": "eslint ./src",
    "setup": "yarn install && flow-typed install testcafe@0.18.4",
    "deps": "madge --image deps.svg --extensions js src"
  },
  "main": "src/index.js",
  "repository": "https://github.com/DmitryFillo/directorcafe",
  "author": "DmitryFillo <fillo@fillo.me>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "3.0.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.3.2",
    "eslint-plugin-testcafe": "0.2.1",
    "flow-bin": "0.47.0",
    "flow-typed": "2.2.3",
    "jest": "21.2.1"
  },
  "dependencies": {
    "log4js": "2.3.12",
    "prettyjson": "1.2.1",
    "testcafe": "0.18.4"
  }
}
